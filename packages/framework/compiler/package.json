{
    "name": "@l8b/compiler",
    "version": "0.0.1",
    "description": "LootiScript compiler utilities and helpers",
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist/**/*",
        "package.json",
        "README.md"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "test": "vitest run --passWithNoTests"
    },
    "dependencies": {
        "@l8b/lootiscript": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    }
}