{
      "name": "@l8b/compiler",
      "version": "1.0.0",
      "description": "LootiScript compiler utilities and helpers",
      "type": "module",
      "sideEffects": false,
      "files": [
            "dist/**/*",
            "package.json",
            "README.md"
      ],
      "exports": {
            ".": "./dist/index.js"
      },
      "scripts": {
            "build": "tsup",
            "clean": "rm -rf dist",
            "typecheck": "tsc --noEmit",
            "test": "vitest run --passWithNoTests"
      },
      "dependencies": {
            "@l8b/lootiscript": "workspace:*",
            "@l8b/diagnostics": "workspace:*"
      },
      "devDependencies": {
            "@types/node": "^20.0.0",
            "tsup": "^8.0.0",
            "typescript": "^5.0.0"
      }
}
