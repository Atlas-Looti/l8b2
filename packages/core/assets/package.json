{
	"name": "@l8b/assets",
	"version": "1.0.5",
	"description": "Asset management system for loading and managing game assets",
	"sideEffects": false,
	"files": [
		"dist/**/*",
		"README.md",
		"package.json"
	],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"dependencies": {
		"@l8b/diagnostics": "workspace:*",
		"@l8b/sprites": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^22.19.1",
		"@vitest/coverage-v8": "^4.0.7",
		"jsdom": "^24.1.3",
		"vitest": "^4.0.9"
	},
	"keywords": [
		"assets",
		"asset-management",
		"game-engine",
		"loader"
	],
	"engines": {
		"node": ">=18"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}
